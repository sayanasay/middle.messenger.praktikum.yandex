{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2020",
    "declaration": false,
    "module": "ESNext",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  }
}
